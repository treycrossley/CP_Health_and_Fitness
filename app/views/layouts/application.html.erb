  <div class="container">
    <div class='row'>
      <div class='col-md-4 float-left'>
        
        <img src="http://www.fit4lifejax.com/wp-content/uploads/2014/09/ace2.png">
        
      </div>
      
      <div class="col-md-4">
        
        <%= image_tag "lets.gif" %>
      </div>
      
      <div class="col-md-4" align="right">
       <br> <p>
        Email me at cindy.perry111@gmail.com<br> or call me at (949) 887-0425 </p>
        
      </div>
    </div>
    
  </div>
  
  
</div>






</body>
</html>
