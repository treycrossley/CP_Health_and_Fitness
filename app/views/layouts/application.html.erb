          <li><%= link_to "Shakeology", 'https://www.shakeology.com/cmperry' %> </li>
        </ul>
      </div>
    </div>
    
    
    <div class="jumbotron">
      <div class="container">
        <h1>Work toward a better you</h1>
        <p>Let me guide you as you start eating healthier, training harder, and living an overall better lifestyle</p>
        
      </div>
    </div> 
    
    
<%= yield %>

